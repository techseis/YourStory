@model IList<ArticleOverviewModel>

@using YStory.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="related-articles-grid article-grid">
        <div class="title">
            <strong>@T("Articles.RelatedArticles")</strong>
        </div>
        <div class="item-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">@Html.Partial("_ArticleBox", item)</div>
            }
        </div>
    </div>
}